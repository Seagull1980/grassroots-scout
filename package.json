{
  "name": "the-grassroots-scout",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "backend": "cd backend && node server.js",
    "team-management-server": "cd backend && node team-management-server.js",
    "team-roster-server": "cd backend && node team-roster-server.js",
    "playing-history-server": "cd backend && node playing-history-server.js",
    "forum-server": "cd backend && node forum-server.js",
    "dev:full": "concurrently \"npm run backend\" \"npm run team-management-server\" \"npm run team-roster-server\" \"npm run playing-history-server\" \"npm run forum-server\" \"npm run dev\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "railway:build": "npm install && npm run build",
    "railway:start": "cd backend && node server.js",
    "start": "cd backend && node server.js",
    "setup-admin": "cd backend && node create-production-admin.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@googlemaps/react-wrapper": "^1.2.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@types/leaflet": "^1.9.21",
    "@vitejs/plugin-react": "^4.1.1",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.5.0",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.5",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.20.1",
    "serve": "^14.2.1",
    "sqlite3": "^5.1.6",
    "typescript": "^5.2.2",
    "uuid": "^13.0.0",
    "vite": "^5.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^24.10.4",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.2.0",
    "terser": "^5.46.0",
    "vitest": "^1.0.4"
  }
}
